
public class MyOop {

	public static void main(String[] args) {
		printA();
		printA();
		
		printB();
		printB();

	}

	private static void printA() {
		System.out.println("----");
		System.out.println("A");
		System.out.println("A");
	}
	private static void printB() {
		System.out.println("----");
		System.out.println("B");
		System.out.println("B");
	}
}
