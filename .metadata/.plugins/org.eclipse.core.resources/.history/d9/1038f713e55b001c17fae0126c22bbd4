
public class MyOop {

	public static String delimiter = ""
	public static void main(String[] args) {
		delimiter = "----";
		printA();
		printA();
		printB();
		printB();
		
		delimiter = "****";
		printA();
		printA();
		printB();
		printB();

	}

	private static void printA(delimiter) {
		System.out.println(delimiter);
		System.out.println("A");
		System.out.println("A");
	}
	private static void printB(delimiter) {
		System.out.println(delimiter);
		System.out.println("B");
		System.out.println("B");
	}
}
